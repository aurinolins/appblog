<h1>Lista de Posts</h1>
    {{#each posts}}
      <div class="body">
        <h1>{{this.dataValues.titulo}}</h1>
        <h2>{{this.dataValues.conteudo}}</h2>
        <p>  Id : </p>
        <h3>{{this.dataValues.id}}</h3>
        <a href="/del/{{this.dataValues.id}}"><buttom>Deletar</buttom> > </a>
        <hr>
      </div>
    {{/each}}


